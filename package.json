		"gulp-babel": "^7.0.1",
		"gulp-browserify": "^0.5.1",
		"gulp-clean-css": "^3.9.3",
		"gulp-minify": "^2.1.0",
		"gulp-rename": "^1.2.2",
		"gulp-sass": "^4.0.1",
		"gulp-sass-import": "^1.0.0",
		"gulp-sourcemaps": "^2.6.4",
		"gulp-typescript": "^4.0.2",
		"gulp-uglify": "^3.0.0",
		"gulp-watch": "^5.0.0",
		"jest": "^22.4.3",
		"merge2": "^1.2.1",
		"pump": "^3.0.0",
